{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.formatOnSave": true,
	"[javascript]": {
		"editor.formatOnSave": false
	},
	"[javascriptreact]": {
		"editor.formatOnSave": false
	},
	"[typescript]": {
		"editor.formatOnSave": false
	},
	"[typescriptreact]": {
		"editor.formatOnSave": false
	},
	"files.exclude": {
		"**/._*": true,
		"node_modules/": true,
		".github/": true,
		".git/": true,
		"LICENSE": true,
		"CONTRIBUTING.md": true,
		"CODE_OF_CONDUCT.md": true,
		".editorconfig": true,
		".eslintignore": true,
		".prettierignore": true,
		".gitignore": true,
		"src/react-app-env.d.ts": true,
}
